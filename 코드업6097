h,w=map(int,input().split())
n=int(input())
Board=[]
for i in range(h):
  Board.append([])
  for j in range(w):
    Board[i].append(0)
for k in range(n):
  i,d,x,y=map(int,input().split())
  for k in range(i):
    if d==0: Board[x-1][y+k-1]=1
    elif d==1:Board[x+k-1][y-1]=1
for i in range(h):
  for j in range(w):
    print(Board[i][j],end=" ")
  print("")
